pmid = {19411603},
URL = {http://genome.cshlp.org/content/19/5/859.long},
local-url = {file://localhost/Volumes/MacintoshHD2/Users/karroje/Documents/papers/2009/Han/Genome%20Res%202009%20Han.pdf},
uri = {papers://A5194097-9461-49E7-8CDA-F038635AA2C3/Paper/p1728},
