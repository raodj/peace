language = {eng},
keywords = {Gene Expression Regulation: Plant, Point Mutation, Mutation, Arabidopsis, Base Sequence, DNA: Intergenic, DNA: Plant, Transcription: Genetic, Genome: Plant, Base Composition, Evolution: Molecular}, 
